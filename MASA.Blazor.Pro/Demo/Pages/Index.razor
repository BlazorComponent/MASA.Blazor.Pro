@namespace MASA.Blazor.Pro.Pages

<MSubheader>Pages</MSubheader>

<MListGroup PrependIcon="fas fa-user-check">
    <ActivatorContent>
        <MListItemTitle Class="subtitle-1">Authentication</MListItemTitle>
    </ActivatorContent>
    <ChildContent>
        <MListItem Link Href="/pages/authentication/Login-v1">
            <MListItemIcon Class="mx-3">
                <MIcon> fas fa-genderless </MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Login v1</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/pages/authentication/login-v2">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Login v2</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/pages/authentication/register-v1">
            <MListItemIcon Class="mx-3">
                <MIcon> fas fa-genderless </MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Register v1</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/pages/authentication/forgot-password-v1">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Forgot Password v1</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/pages/authentication/forgot-password-v2">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Forgot Password v2</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/pages/authentication/reset-password-v1">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Reset Password v1</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/pages/authentication/reset-password-v2">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Reset Password v2</MListItemTitle>
            </MListItemContent>
        </MListItem>
    </ChildContent>
</MListGroup>

<MListItem Link Href="/pages/account-settings">
    <MListItemIcon>
        <MIcon>fas fa-user-cog</MIcon>
    </MListItemIcon>
    <MListItemContent  Style="margin-left:-6px">
        <MListItemTitle Class="subtitle-1">Account Settings</MListItemTitle>
    </MListItemContent>
</MListItem>

<MListGroup PrependIcon="fas fa-exclamation-circle">
    <ActivatorContent>
        <MListItemTitle Class="subtitle-1">Miscellaneous</MListItemTitle>
    </ActivatorContent>
    <ChildContent>
        <MListItem Link Href="/pages/miscellaneous/not-authorized">
            <MListItemIcon Class="mx-3">
                <MIcon> fas fa-genderless </MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Not Authorized</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/pages/miscellaneous/error">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Error</MListItemTitle>
            </MListItemContent>
        </MListItem>
    </ChildContent>
</MListGroup>

