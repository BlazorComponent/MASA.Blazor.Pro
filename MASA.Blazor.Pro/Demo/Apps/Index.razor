@namespace MASA.Blazor.Pro.Apps

<MSubheader>Apps</MSubheader>


<MListItem Link Href="/apps/todo">
    <MListItemIcon>
        <MIcon>fas fa-check-square</MIcon>
    </MListItemIcon>
    <MListItemContent>
        <MListItemTitle Class="subtitle-1">Todo</MListItemTitle>
    </MListItemContent>
</MListItem>

<MListGroup PrependIcon="fas fa-receipt">
    <ActivatorContent>
        <MListItemTitle Class="subtitle-1">Invoice</MListItemTitle>
    </ActivatorContent>
    <ChildContent>
        <MListItem Link Href="/apps/invoice/list">
            <MListItemIcon Class="mx-3">
                <MIcon> fas fa-genderless </MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">List</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/apps/invoice/preview">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Preview</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/apps/invoice/edit">
            <MListItemIcon Class="mx-3">
                <MIcon> fas fa-genderless </MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Edit</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/apps/invoice/add">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Add</MListItemTitle>
            </MListItemContent>
        </MListItem>
    </ChildContent>
</MListGroup>

<MListItem Link Href="/apps/ecommerce">
    <MListItemIcon>
        <MIcon>fas fa-shopping-bag</MIcon>
    </MListItemIcon>
    <MListItemContent>
        <MListItemTitle Class="subtitle-1">eCommerce</MListItemTitle>
    </MListItemContent>
</MListItem>

<MListGroup PrependIcon="far fa-user">
    <ActivatorContent>
        <MListItemTitle Class="subtitle-1">User</MListItemTitle>
    </ActivatorContent>
    <ChildContent>
        <MListItem Link Href="/apps/user/list">
            <MListItemIcon Class="mx-3">
                <MIcon> fas fa-genderless </MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">List</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/apps/user/view">
            <MListItemIcon Class="mx-3">
                <MIcon>fas fa-genderless</MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">View</MListItemTitle>
            </MListItemContent>
        </MListItem>
        <MListItem Link Href="/apps/user/edit">
            <MListItemIcon Class="mx-3">
                <MIcon> fas fa-genderless </MIcon>
            </MListItemIcon>
            <MListItemContent Class="mx-3">
                <MListItemTitle Class="subtitle-1">Edit</MListItemTitle>
            </MListItemContent>
        </MListItem>
    </ChildContent>
</MListGroup>


